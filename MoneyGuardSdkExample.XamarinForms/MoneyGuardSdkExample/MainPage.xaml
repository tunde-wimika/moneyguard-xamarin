<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MoneyGuardSdkExample.MainPage">

    <StackLayout>
        <Frame BackgroundColor="#7C4DFF" Padding="24" CornerRadius="0">
            <Label Text="Wimika RMS Moneyguard SDK Embedding" HorizontalTextAlignment="Center" TextColor="White" FontSize="36"/>
        </Frame>
        <Label Text="Session Id" FontSize="Title" Padding="30,10,30,10"/>
        <Label Text="{Binding Session.SessionId}" FontSize="16" Padding="30,0,30,0"/>
        <Label Text="Installation Id" FontSize="Title" Padding="30,10,30,10"/>
        <Label Text="{Binding Session.InstallationId}" FontSize="16" Padding="30,0,30,0"/>
        
    </StackLayout>

</ContentPage>
